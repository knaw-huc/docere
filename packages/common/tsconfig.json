{
	"compilerOptions": {
		"declaration": true,
		"declarationMap": true,
		"esModuleInterop": true,
		"importHelpers": true,
		"incremental": true,
		"isolatedModules": true,
		"jsx": "react",
		"module": "commonjs",
		"moduleResolution": "node",
		"noImplicitAny": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"outDir": "./build",
		"removeComments": true,
		"resolveJsonModule": true,
		"target": "esnext",
		"types": []
	},
	"exclude": [
		"build",
		"node_modules"
	],
	"include": [
		"./src/index.ts",
		"./src/global.d.ts"
	]
}
