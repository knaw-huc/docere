{
	"compilerOptions": {
		"declaration": true,
		"declarationMap": true,
		"esModuleInterop": true,
		"importHelpers": true,
		"incremental": true,
		"isolatedModules": true,
		"jsx": "react",
		"module": "esnext",
		"moduleResolution": "node",
		"noImplicitAny": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"outDir": "./build/types",
		"preserveConstEnums": true,
		"removeComments": true,
		"resolveJsonModule": true,
		"target": "esnext",
		"types": []
	},
	"include": ["./src/index.tsx", "../common/src/global.d.ts"],
	"exclude": ["./node_modules", "./build", "./dist"]
}
